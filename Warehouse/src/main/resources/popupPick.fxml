        </Label>
        <Label layoutX="63.0" layoutY="93.0" text="เลขที่คลัง">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <ComboBox fx:id="unitCombo" layoutX="274.0" layoutY="173.0" prefHeight="31.0" prefWidth="98.0" promptText="Unit" />
        <Button fx:id="addBtn" layoutX="123.0" layoutY="255.0" mnemonicParsing="false" onAction="#handlerBtnAdd" text="เพิ่มลงตาราง" />
        <Button fx:id="cancelBtn" layoutX="216.0" layoutY="255.0" mnemonicParsing="false" text="ยกเลิก" />
